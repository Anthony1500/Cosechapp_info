    <style name="Theme.Cosechapp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Cosechapp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
